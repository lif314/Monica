{
  "name": "monica",
  "version": "0.4.5",
  "description": "Video-based motion capture system.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "start-nogpu": "electron . --disable-gpu",
    "package:mac64": "electron-packager ./ SysMocap --platform=darwin --arch=x64 --out ./OutApp --download.mirrorOptions.mirror=https://npm.taobao.org/mirrors/electron/ --overwrite --usage-description.Camera='该程序需要摄像头权限' --electronVersion 19.0.0-beta.8 --asar",
    "package:macarm": "electron-packager ./ SysMocap --platform=darwin --arch=arm64 --out ./OutApp --download.mirrorOptions.mirror=https://npm.taobao.org/mirrors/electron/ --overwrite --usage-description.Camera='该程序需要摄像头权限' --electronVersion 19.0.0-beta.8 --asar",
    "package:win64": "electron-packager ./ SysMocap --icon=sysmocap.ico --platform=win32 --arch=x64 --out ./OutApp --download.mirrorOptions.mirror=https://npm.taobao.org/mirrors/electron/ --overwrite --electronVersion 19.0.0-beta.8 --asar",
    "zip:win64": "electron-installer-zip ./OutApp/SysMocap-win32-x64/ ./OutApp/packages/SysMocap-Windows-x64.zip --overwrite",
    "zip:mac64": "electron-installer-zip ./OutApp/SysMocap-darwin-x64/ ./OutApp/packages/SysMocap-macOS-x64.zip --overwrite",
    "dmg": "electron-installer-dmg ./OutApp/SysMocap-darwin-x64/SysMocap.app SysMocap-macOS-x64 --out ./OutApp/packages/ --overwrite",
    "dmgarm": "electron-installer-dmg ./OutApp/SysMocap-darwin-arm64/SysMocap.app SysMocap-macOS-aarch64 --out ./OutApp/packages/ --overwrite"
  },
  "author": "Linfei Li",
  "license": "ISC",
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@mediapipe/camera_utils": "^0.3.1640029074",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/holistic": "^0.5.1635989137",
    "@pixiv/three-vrm": "^0.6.10",
    "command-line-usage": "^6.1.3",
    "electron-acrylic-window": "^0.5.11",
    "electron-find": "^1.0.7",
    "electron-localstorage": "^1.0.5",
    "express": "^4.17.3",
    "express-ws": "^5.0.2",
    "figlet": "^1.5.2",
    "jquery": "^3.6.0",
    "jsoneditor": "^9.7.4",
    "kalidokit": "^1.1.5",
    "lato-font": "^3.0.0",
    "lil-gui": "^0.16.1",
    "marked": "^4.0.12",
    "mdui": "^1.0.2",
    "pdfjs-dist": "^2.13.216",
    "simple-argv": "^2.0.4",
    "socket.io": "^4.5.0",
    "three": "^0.137.4",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "electron": "^19.0.0-beta.8",
    "electron-installer-dmg": "^4.0.0",
    "electron-installer-zip": "^0.1.2",
    "electron-packager": "^15.4.0"
  }
}
